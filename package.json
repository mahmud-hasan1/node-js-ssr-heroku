{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "16.x"
  },
  "main": "server.js",
  "scripts": {
    "build": "set NodeENV=production&webpack",
    "build:dev": "webpack -w",
    "server:run": "nodemon --watch build --exec \"node build/n/server.js \" ",
    "dev": "set NodeENV=development&npm-run-all --parallel build:dev server:run",
    "start": "node build/n/server.js",
    "test": "node test.js"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "classnames": "^2.3.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-topbar-progress-indicator": "^4.1.0",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
